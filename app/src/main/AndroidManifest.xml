<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.github.leibnik.about_draw"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.github.leibnik.about_draw.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="io.github.leibnik.about_draw.ScreenTest"/>
        <activity android:name="io.github.leibnik.about_draw.CanvasProTest"/>
        <activity android:name="io.github.leibnik.about_draw.CanvasBaseTest"/>
        <activity android:name="io.github.leibnik.about_draw.ColorMatrixTest"/>
        <activity android:name="io.github.leibnik.about_draw.MatrixTest"/>
        <activity android:name="io.github.leibnik.about_draw.PaintTest"/>
        <activity android:name="io.github.leibnik.about_draw.XmlTest"/>
        <activity android:name="io.github.leibnik.about_draw.SurfaceViewTest"/>
    </application>

</manifest>
